/* footer styles */
#footer {
  background: #4e5558;
  padding: 295px 0 90px;
  color: #adadad;
  font-size: 14px;
  line-height: 24px;
  position: relative;
  z-index: 3;
  overflow: hidden;
  margin: -70px 0 0;
  color: #979999;
}
#footer:after {
  content: '\e62f';
  position: absolute;
  left: 50%;
  top: 200px;
  font-family: icomoon, sans-serif;
  margin: 0 0 0 -235px;
  font-size: 510px;
  line-height: 500px;
  z-index: -1;
  color: white;
  opacity: 0.02;
}
#footer .logo {
  float: none;
  margin: -13px 0 35px;
}
#footer h3 {
  color: #fff;
  font: bold 16px/24px "brandon-grotesque", Helvetica, Arial, sans-serif;
  text-transform: uppercase;
  margin: 0 0 30px;
}
#footer .footer-nav {
  font-size: 17px;
}
#footer .footer-nav li a {
  color: #adadad;
}
#footer .footer-nav li a:hover {
  color: #c7c7c7;
}
#footer .tools {
  font-size: 14px;
}
#footer .tools li {
  float: left;
  margin: 0 10px 0 0;
}
#footer .tools a {
  color: #7d8183;
}
#footer .tools a:hover {
  color: #979a9c;
}
#footer .social-networks {
  line-height: 30px;
  margin: 0 0 20px;
  font-family: Socialico, sans-serif;
}
#footer .social-networks li {
  float: left;
  margin: 0 11px 0 0;
}
#footer .social-networks a {
  display: block;
  font-size: 42px;
  line-height: 42px;
  color: #adadad;
  -webkit-transition: color .2s linear;
  -moz-transition: color .2s linear;
  transition: color .2s linear;
}
#footer .social-networks a:hover {
  color: #c7c7c7;
}
#footer .overlay-top {
  position: absolute;
  width: 3000px;
  height: 300px;
  left: 50%;
  top: -150px;
  -moz-transform: rotate(5deg);
  -ms-transform: rotate(5deg);
  -o-transform: rotate(5deg);
  -webkit-transform: rotate(5deg);
  margin: 0 0 -76px -1500px;
  background: white;
}
#footer p {
  margin: 0 0 7px;
}
@media screen and (max-width: 64em) {
  #footer {
    margin: 0;
    padding: 150px 0 40px;
  }
  #footer .overlay-top {
    height: 150px;
    top: -75px;
  }
  #footer .social-networks li {
    margin: 0 3px 0 0;
  }
}
@media screen and (max-width: 47.8em) {
  #footer {
    text-align: center;
    font-size: 12px;
    padding: 75px 10%  47px;
    margin: 0;
  }
  #footer .overlay-top {
    background: none;
  }
  #footer .logo {
    margin: 0 auto 30px;
  }
  #footer .tools {
    display: none;
  }
  #footer:after {
    display: none;
  }
  #footer p {
    margin: 0 0 14px;
  }
  #footer h3 {
    font-size: 16px;
    line-height: 22px;
    margin: 0 0 10px;
  }
  #footer h3.add {
