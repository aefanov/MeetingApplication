  display: block;
  font-size: 17px;
  line-height: 22px;
  color: #1a171b;
}
.btn-get-started:after {
  content: '\e604';
  font-family: icomoon, sans-serif;
  position: absolute;
  right: 36px;
  top: 50%;
  font-size: 46px;
  line-height: 46px;
  margin: -23px 0 0;
  color: white;
  opacity: 0.5;
}
@media screen and (max-width: 64em) {
